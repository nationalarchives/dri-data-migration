# Object properties

:subsetHasBroaderSubset a owl:ObjectProperty;
	rdfs:domain :Subset;
	rdfs:range :Subset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Subset has broader subset".

:subsetHasNarrowerSubset a owl:ObjectProperty;
	owl:inverseOf :subsetHasBroaderSubset;
	rdfs:domain :Subset;
	rdfs:range :Subset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Subset has narrower subset".

:subsetHasAsset a owl:ObjectProperty, owl:InverseFunctionalProperty;
	rdfs:domain :Subset;
	rdfs:range :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Subset has asset".

:assetHasSubset a owl:ObjectProperty, owl:FunctionalProperty;
	owl:inverseOf :subsetHasAsset;
	rdfs:domain :Asset;
	rdfs:range :Subset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset has subset".

:assetHasLanguage a owl:ObjectProperty;
	rdfs:seeAlso rico:hasOrHadLanguage;
	rdfs:domain :Asset;
	rdfs:range :Language;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset has language".

:languageHasAsset a owl:ObjectProperty;
	owl:inverseOf :assetHasLanguage;
	rdfs:seeAlso rico:isOrWasLanguageOf;
	rdfs:domain :Language;
	rdfs:range :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Language has asset".

:assetHasCopyright a owl:ObjectProperty;
	rdfs:seeAlso rico:hasOrHadAllMembersWithLegalStatus;
	rdfs:domain :Asset;
	rdfs:range :Copyright;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset has copyright".

:copyrightHasAsset a owl:ObjectProperty;
	owl:inverseOf :assetHasCopyright;
	rdfs:seeAlso rico:isOrWasLegalStatusOfAllMembersOf;
	rdfs:domain :Copyright;
	rdfs:range :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Copyright has asset".

:assetHasLegalStatus a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:seeAlso rico:hasOrHadAllMembersWithLegalStatus;
	rdfs:domain :Asset;
	rdfs:range :LegalStatus;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset has legal status".

:legalStatusHasAsset a owl:ObjectProperty, owl:InverseFunctionalProperty;
	owl:inverseOf :assetHasLegalStatus;
	rdfs:seeAlso rico:isOrWasLegalStatusOfAllMembersOf;
	rdfs:domain :LegalStatus;
	rdfs:range :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Legal status has asset".

:assetHasAssociatedGeographicalPlace a owl:ObjectProperty;
	rdfs:domain :Asset;
	rdfs:range :GeographicalPlace;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset has associated geographical place".

:associatedGeographicalPlaceHasAsset a owl:ObjectProperty;
	owl:inverseOf :assetHasAssociatedGeographicalPlace;
	rdfs:domain :GeographicalPlace;
	rdfs:range :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Associated geographical place has asset".

:assetHasChange a owl:ObjectProperty, owl:InverseFunctionalProperty;
	rdfs:domain :Asset;
	rdfs:range :Change;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset has change".

:changeHasAsset a owl:ObjectProperty;
	owl:inverseOf :assetHasChange;
