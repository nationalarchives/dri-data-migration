@prefix :<http://id.example.com/schema/>.
@prefix this:<http://id.example.com/>.
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl:<http://www.w3.org/2002/07/owl#>.
@prefix xsd:<http://www.w3.org/2001/XMLSchema#>.
@prefix rico:<https://www.ica.org/standards/RiC/ontology#>.

this:schema a owl:Ontology;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Digital preservation ontology.".


# Types

:ExternalThing a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:label "External thing".

:NamedThing a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Named thing".

:Subset a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:seeAlso rico:RecordSet;
	rdfs:label "Subset";
	rdfs:comment "Group of :Subset or :Asset.".

:Asset a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:seeAlso rico:Record;
	rdfs:label "Asset";
	rdfs:comment "A thing describing archived resource.".

:InquiryAsset a owl:Class;
	rdfs:subClassOf :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Inquiry asset".

:CourtAsset a owl:Class;
	rdfs:subClassOf :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Court asset".

:Variation a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:seeAlso rico:Instantiation;
	rdfs:label "Variation";
	rdfs:comment "A variant of an :Asset.".

:RedactedVariation a owl:Class;
	rdfs:subClassOf :Variation;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Redacted variation";
	rdfs:comment "A version of a :Variant with sensitive information hidden. More details at https://cdn.nationalarchives.gov.uk/documents/information-management/redaction-considerations.pdf and https://cdn.nationalarchives.gov.uk/documents/information-management/redaction_toolkit.pdf.".

:AlternativeVariation a owl:Class;
	rdfs:subClassOf :Variation;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Alternative variation";
	rdfs:comment "An alternative version of a :Variant.".

:Language a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:seeAlso rico:Language;
	rdfs:label "Language";
	rdfs:comment "Language of the :Asset.".

:Language a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:seeAlso rico:Language;
	rdfs:label "Language";
	rdfs:comment "Language of the :Asset.".

:Copyright a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:seeAlso rico:conditionsOfUse;
	rdfs:label "Copyright";
	rdfs:comment "Copyright of the :Asset. More details at https://cdn.nationalarchives.gov.uk/documents/information-management/copyright-related-rights.pdf and https://cdn.nationalarchives.gov.uk/documents/information-management/use-of-tna-materials.pdf.".

:LegalStatus a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:seeAlso rico:LegalStatus;
	rdfs:label "Legal status";
	rdfs:comment "Legal status of the :Asset. More details at https://www.nationalarchives.gov.uk/information-management/legislation/public-records-act/pra-faqs/.".

:PublicRecord a owl:Class;
	rdfs:subClassOf :LegalStatus;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Public record".

:NotPublicRecord a owl:Class;
	rdfs:subClassOf :LegalStatus;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Not public record".

:WelshPublicRecord a owl:Class;
	rdfs:subClassOf :LegalStatus;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Welsh public record";
	rdfs:comment "More details at https://www.nationalarchives.gov.uk/information-management/legislation/public-records-act/concordat-welsh-public-records/.".

:NonRecordMaterial a owl:Class;
	rdfs:subClassOf :LegalStatus;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Non-record material".

:FormalBody a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:seeAlso rico:Agent;
	rdfs:label "Formal body";
	rdfs:comment "An entity (e.g. government department, public institution) that creates or holds a thing.".

:Custodianship a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Custodianship";
	rdfs:comment "An activity of being in possession of a thing.".

:PastCustodianship a owl:Class;
	rdfs:subClassOf :Custodianship;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Past custodianship";
	rdfs:comment "A :Custodianship has ended.".

:Creation a owl:Class;
	rdfs:subClassOf :Custodianship;
	rdfs:isDefinedBy this:schema;
	rdfs:seeAlso rico:CreationRelation;
	rdfs:label "Creation";
	rdfs:comment "A relation describing creation of a thing by a :FormalBody.".

:Retention a owl:Class;
	rdfs:subClassOf :Custodianship;
	rdfs:isDefinedBy this:schema;
	rdfs:seeAlso rico:RecordResourceHoldingRelation;
	rdfs:label "Retention";
	rdfs:comment "A relation describing retention of a thing by a :FormalBody.".

:AccessCondition a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Access condition";
	rdfs:comment "A thing describing type of access.".

:SensitivityReviewRestriction a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:seeAlso rico:Rule;
	rdfs:label "Sensitivity review restriction";
	rdfs:comment "Details of restrictions applied to a thing which are an outcome of a :SensitivityReview.".

:RetentionRestriction a owl:Class;
	rdfs:subClassOf :SensitivityReviewRestriction;
	rdfs:isDefinedBy this:schema;
	rdfs:seeAlso rico:Rule;
	rdfs:label "Retention restriction";
	rdfs:comment "Type of :SensitivityReviewRestriction resulting in retention of a thing.".

:GroundForRetention a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Ground for retention";
	rdfs:comment "Justification of retention (details: https://cdn.nationalarchives.gov.uk/documents/information-management/retention.pdf) as per application to the Advisory Council (https://www.nationalarchives.gov.uk/about/our-role/advisory-council/).".

:Legislation a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Legislation".

:UkLegislation a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:label "UK legislation (provided by https://www.legislation.gov.uk/)".

:SensitivityReview a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:seeAlso rico:Activity;
	rdfs:label "Sensitivity review";
	rdfs:comment "An activity performed on :Asset to establish whenever an :Asset is open to public or kept private.".

:PastSensitivityReview a owl:Class;
	rdfs:subClassOf :SensitivityReview;
	rdfs:isDefinedBy this:schema;
	rdfs:seeAlso rico:Activity;
	rdfs:label "Past sensitivity review".

:CourtCase a owl:Class;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Court case".


# Object properties

:subsetHasBroaderSubset a owl:ObjectProperty;
	rdfs:domain :Subset;
	rdfs:range :Subset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Subset has broader subset".

:subsetHasNarrowerSubset a owl:ObjectProperty;
	owl:inverseOf :subsetHasBroaderSubset;
	rdfs:domain :Subset;
	rdfs:range :Subset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Subset has narrower subset".

:subsetHasAsset a owl:ObjectProperty, owl:InverseFunctionalProperty;
	rdfs:domain :Subset;
	rdfs:range :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Subset has asset".

:assetHasSubset a owl:ObjectProperty, owl:FunctionalProperty;
	owl:inverseOf :subsetHasAsset;
	rdfs:domain :Asset;
	rdfs:range :Subset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset has subset".

:assetHasLanguage a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:seeAlso rico:hasOrHadLanguage;
	rdfs:domain :Asset;
	rdfs:range :Language;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset has language".

:languageHasAsset a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:seeAlso rico:isOrWasLanguageOf;
	rdfs:domain :Language;
	rdfs:range :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Language has asset".

:assetHasCopyright a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:seeAlso rico:hasOrHadAllMembersWithLegalStatus;
	rdfs:domain :Asset;
	rdfs:range :Copyright;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset has copyright".

:copyrightHasAsset a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:seeAlso rico:isOrWasLegalStatusOfAllMembersOf;
	rdfs:domain :Copyright;
	rdfs:range :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Copyright has asset".

:assetHasLegalStatus a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:seeAlso rico:hasOrHadAllMembersWithLegalStatus;
	rdfs:domain :Asset;
	rdfs:range :LegalStatus;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset has legal status".

:legalStatusHasAsset a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:seeAlso rico:isOrWasLegalStatusOfAllMembersOf;
	rdfs:domain :LegalStatus;
	rdfs:range :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Legal status has asset".

:assetHasCourtCase a owl:ObjectProperty;
	rdfs:domain :Asset;
	rdfs:range :CourtCase;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset has court case".

:courtCaseHasAsset a owl:ObjectProperty;
	rdfs:domain :CourtCase;
	rdfs:range :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Court case has asset".

:assetHasVariation a owl:ObjectProperty, owl:InverseFunctionalProperty;
	rdfs:seeAlso rico:hasOrHadInstantiation;
	rdfs:domain :Asset;
	rdfs:range :Variation;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset has variation".

:variationHasAsset a owl:ObjectProperty, owl:FunctionalProperty;
	owl:inverseOf :assetHasVariation;
	rdfs:seeAlso rico:isOrWasInstantiationOf;
	rdfs:domain :Variation;
	rdfs:range :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Variation has asset".

:variationHasRedactedVariation a owl:ObjectProperty;
	rdfs:seeAlso rico:migratedInto;
	rdfs:domain :Variation;
	rdfs:range :RedactedVariation;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Variation has redacted variation".

:redactedVariationHasVariation a owl:ObjectProperty;
	rdfs:seeAlso rico:migratedFrom;
	rdfs:domain :RedactedVariation;
	rdfs:range :Variation;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Redacted variation has variation".

:variationHasAlternativeVariation a owl:ObjectProperty;
	rdfs:seeAlso rico:migratedInto;
	rdfs:domain :Variation;
	rdfs:range :AlternativeVariation;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Variation has alternative variation".

:alternativeVariationHasVariation a owl:ObjectProperty;
	rdfs:seeAlso rico:migratedFrom;
	rdfs:domain :AlternativeVariation;
	rdfs:range :Variation;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Alternative variation has variation".

:creationHasAsset a owl:ObjectProperty;
	rdfs:seeAlso rico:relationHasSource;
	rdfs:domain :Creation;
	rdfs:range :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Creation has asset".

:assetHasCreation a owl:ObjectProperty;
	rdfs:seeAlso rico:thingIsSourceOfRelation;
	rdfs:domain :Asset;
	rdfs:range :Creation;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset has creation".

:creationHasSubset a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:seeAlso rico:relationHasSource;
	rdfs:domain :Creation;
	rdfs:range :Subset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Creation has subset".

:creationHasFormalBody a owl:ObjectProperty;
	rdfs:seeAlso rico:relationHasTarget;
	rdfs:domain :Creation;
	rdfs:range :FormalBody;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Creation has formal body".

:retentionHasSubset a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:seeAlso rico:relationHasTarget;
	rdfs:domain :Retention;
	rdfs:range :Subset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Retention has subset".

:subsetHasRetention a owl:ObjectProperty, owl:FunctionalProperty;
	owl:inverseOf :retentionHasSubset;
	rdfs:domain :Subset;
	rdfs:range :Retention;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Subset has retention".

:retentionHasAsset a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:seeAlso rico:relationHasTarget;
	rdfs:domain :Retention;
	rdfs:range :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Retention has asset".

:assetHasRetention a owl:ObjectProperty, owl:FunctionalProperty;
	owl:inverseOf :retentionHasAsset;
	rdfs:domain :Asset;
	rdfs:range :Retention;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset has retention".

:retentionHasFormalBody a owl:ObjectProperty;
	rdfs:seeAlso rico:relationHasSource;
	rdfs:domain :Retention;
	rdfs:range :FormalBody;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Retention has formal body".

:sensitivityReviewHasAccessCondition a owl:ObjectProperty;
	rdfs:domain :SensitivityReview;
	rdfs:range :AccessCondition;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Sensitivity review has access condition".

:sensitivityReviewHasSensitivityReviewRestriction a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:domain :SensitivityReview;
	rdfs:range :SensitivityReviewRestriction;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Sensitivity review has sensitivity review restriction".

:sensitivityReviewHasPastSensitivityReview a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:domain :SensitivityReview;
	rdfs:range :PastSensitivityReview;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Sensitivity review has past sensitivity review".

:assetHasSensitivityReview a owl:ObjectProperty, owl:InverseFunctionalProperty;
	rdfs:domain :Asset;
	rdfs:range :SensitivityReview;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset has sensitivity review".

:sensitivityReviewHasAsset a owl:ObjectProperty, owl:FunctionalProperty;
	owl:inverseOf :assetHasSensitivityReview;
	rdfs:domain :SensitivityReview;
	rdfs:range :Asset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Sensitivity review has asset".

:variationHasSensitivityReview a owl:ObjectProperty, owl:InverseFunctionalProperty;
	rdfs:domain :Variation;
	rdfs:range :SensitivityReview;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Variation has Sensitivity review".

:sensitivityReviewHasVariation a owl:ObjectProperty, owl:FunctionalProperty;
	owl:inverseOf :variationHasSensitivityReview;
	rdfs:domain :SensitivityReview;
	rdfs:range :Variation;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Sensitivity review has variation".

:sensitivityReviewHasSubset a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:domain :SensitivityReview;
	rdfs:range :Subset;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Sensitivity review has subset".

:subsetHasSensitivityReview a owl:ObjectProperty, owl:InverseFunctionalProperty;
	owl:inverseOf :sensitivityReviewHasSubset;
	rdfs:domain :Subset;
	rdfs:range :SensitivityReview;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Subset has sensitivity review".
	
:sensitivityReviewRestrictionHasLegislation a owl:ObjectProperty;
	rdfs:domain :SensitivityReviewRestriction;
	rdfs:range :Legislation;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Sensitivity review restriction has legislation".

:sensitivityReviewRestrictionHasRetentionRestriction a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:domain :SensitivityReviewRestriction;
	rdfs:range :RetentionRestriction;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Sensitivity review restriction has retention restriction".

:retentionRestrictionHasSensitivityReviewRestriction a owl:ObjectProperty, owl:FunctionalProperty;
	owl:inverseOf :sensitivityReviewRestrictionHasRetentionRestriction;
	rdfs:domain :RetentionRestriction;
	rdfs:range :SensitivityReviewRestriction;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Retention restriction has sensitivity review restriction".

:retentionRestrictionHasGroundForRetention a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:domain :RetentionRestriction;
	rdfs:range :GroundForRetention;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Retention restriction has ground for retention".

:legislationHasUkLegislation a owl:ObjectProperty;
	rdfs:domain :Legislation;
	rdfs:range :UkLegislation;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Legislation has UK legislation".

:retentionHasRetentionRestriction a owl:ObjectProperty, owl:FunctionalProperty;
	rdfs:domain :Retention;
	rdfs:range :RetentionRestriction;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Retention has retention restriction".

:retentionRestrictionHasRetention a owl:ObjectProperty, owl:FunctionalProperty;
	owl:inverseOf :retentionHasRetentionRestriction;
	rdfs:domain :RetentionRestriction;
	rdfs:range :Retention;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Retention restriction has retention".


## Data type properties

:externalIdentifier a owl:DatatypeProperty;
	rdfs:domain :ExternalThing;
	rdfs:range xsd:string;
	rdfs:isDefinedBy this:schema;
	rdfs:label "External identifier".

:name a owl:DatatypeProperty;
	rdfs:domain :NamedThing;
	rdfs:range xsd:string;
	rdfs:subPropertyOf rdfs:label;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Name".

:subsetName a owl:DatatypeProperty;
	rdfs:domain :Subset;
	rdfs:range xsd:string;
	rdfs:subPropertyOf :name;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Subset name".

:subsetReference a owl:DatatypeProperty, owl:FunctionalProperty;
	rdfs:domain :Subset;
	rdfs:range xsd:string;
	rdfs:subPropertyOf :externalIdentifier;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Subset reference";
	rdfs:comment "Unique reference of the :Subset. It is a catalogue reference.".

:assetName a owl:DatatypeProperty;
	rdfs:domain :Asset;
	rdfs:range xsd:string;
	rdfs:subPropertyOf :name;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset name".

:assetDescription a owl:DatatypeProperty;
	rdfs:domain :Asset;
	rdfs:range xsd:string;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset description".

:assetReference a owl:DatatypeProperty, owl:FunctionalProperty;
	rdfs:domain :Asset;
	rdfs:range xsd:string;
	rdfs:subPropertyOf :externalIdentifier;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset reference".

:assetDriId a owl:DatatypeProperty, owl:FunctionalProperty;
	rdfs:domain :Asset;
	rdfs:range xsd:string;
	rdfs:subPropertyOf :externalIdentifier;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset DRI Id".

:assetDriXml a owl:DatatypeProperty;
	rdfs:domain :Asset;
	rdfs:range xsd:base64Binary;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset DRI XML".

:batchDriId a owl:DatatypeProperty, owl:FunctionalProperty;
	rdfs:domain :Asset;
	rdfs:range xsd:string;
	rdfs:subPropertyOf :externalIdentifier;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Batch DRI Id".

:consignmentTdrId a owl:DatatypeProperty, owl:FunctionalProperty;
	rdfs:domain :Asset;
	rdfs:range xsd:string;
	rdfs:subPropertyOf :externalIdentifier;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Consignment TDR Id".

:assetRelationDescription a owl:DatatypeProperty;
	rdfs:domain :Asset;
	rdfs:range xsd:string;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset relation description";
	rdfs:comment "This is in lieu of the actual relationship since the source data is a free text field.".

:assetPhysicalDescription a owl:DatatypeProperty;
	rdfs:seeAlso rico:physicalCharacteristicsNote;
	rdfs:domain :Asset;
	rdfs:range xsd:string;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Physical description".

:assetUseRestrictionDescription a owl:DatatypeProperty;
	rdfs:seeAlso rico:conditionsOfUse;
	rdfs:domain :Asset;
	rdfs:range xsd:string;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Asset use restriction description".

:evidenceProviderName a owl:DatatypeProperty;
	rdfs:domain :InquiryAsset;
	rdfs:range xsd:string;
	rdfs:subPropertyOf :name;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Evidence provider name";
	rdfs:comment "This may need to be remodelled into the entity relationship after looking at the ingested data.".

:investigationName a owl:DatatypeProperty;
	rdfs:domain :InquiryAsset;
	rdfs:range xsd:string;
	rdfs:subPropertyOf :name;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Investigation name";
	rdfs:comment "This may need to be remodelled into the entity relationship after looking at the ingested data.".

:formalBodyName a owl:DatatypeProperty, owl:FunctionalProperty;
	rdfs:domain :FormalBody;
	rdfs:range xsd:string;
	rdfs:subPropertyOf :name;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Formal body name".

:custodianshipStartAt a owl:DatatypeProperty;
	rdfs:domain :Custodianship;
	rdfs:range xsd:dateTime;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Custodianship start at".

:custodianshipEndDate a owl:DatatypeProperty;
	rdfs:domain :PastCustodianship;
	rdfs:range xsd:date;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Custodianship end date".

:importLocation a owl:DatatypeProperty;
	rdfs:domain :Retention;
	rdfs:range xsd:string;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Import location".

:variationDriId a owl:DatatypeProperty, owl:FunctionalProperty;
	rdfs:domain :Variation;
	rdfs:range xsd:string;
	rdfs:subPropertyOf :externalIdentifier;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Variation DRI Id".

:variationName a owl:DatatypeProperty;
	rdfs:domain :Variation;
	rdfs:range xsd:string;
	rdfs:subPropertyOf :name;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Variation name".

:variationPastName a owl:DatatypeProperty;
	rdfs:domain :Variation;
	rdfs:range xsd:string;
	rdfs:subPropertyOf :name;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Variation past name";
	rdfs:comment "This is in lieu of previous names relationship.".

:variationDriXml a owl:DatatypeProperty;
	rdfs:domain :Variation;
	rdfs:range xsd:base64Binary;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Variation DRI XML".

:variationNote a owl:DatatypeProperty;
	rdfs:domain :Variation;
	rdfs:range xsd:string;
	rdfs:subPropertyOf rdfs:comment;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Variation note".

:variationRelativeLocation a owl:DatatypeProperty, owl:FunctionalProperty;
	rdfs:domain :Variation;
	rdfs:range xsd:anyURI;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Variation relative location".

:variationPhysicalConditionDescription a owl:DatatypeProperty;
	rdfs:seeAlso rico:qualityOfRepresentationNote;
	rdfs:domain :Variation;
	rdfs:range xsd:string;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Variation physical condition description".

:sensitivityReviewDate a owl:DatatypeProperty;
	rdfs:domain :SensitivityReview;
	rdfs:range xsd:date;
	rdfs:isDefinedBy this:schema;
	rdfs:label "Sensitivity review date".

:sensitivityReviewSensitiveName a owl:DatatypeProperty;
	rdfs:domain :SensitivityReview;
	rdfs:range xsd:string;
	rdfs:subPropertyOf :name;
