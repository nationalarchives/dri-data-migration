@model IEnumerable<Models.Retention>

@if (Model.Any())
{
    @if (Model.Single().RetentionBody.Any())
    {
        <dt>Retained by</dt>
        <dd>@Model.Single().RetentionBody.Single().Name.Single()</dd>
    }
    <partial name="DynamicCollectionTextPartial" model="@("Retention import location", Model.Single().ImportLocation)" />
}